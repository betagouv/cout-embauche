{
  "rules": {
    "indent": [
      2,
      "tab"
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "never"
    ]
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true
  },
  "extends": "eslint:recommended"
}
